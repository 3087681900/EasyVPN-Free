<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/homeContextRL"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.vasilkoff.easyvpnfree.activity.HomeActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.casper.app.activity.HomeActivity"/>

    <Button
        android:id="@+id/homeBtnChooseCountry"
        android:text="@string/home_btn_choose_country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/buttons"
        android:minHeight="40dp"
        android:minWidth="150dp"
        android:layout_marginBottom="25dp"
        android:onClick="homeOnClick"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>

